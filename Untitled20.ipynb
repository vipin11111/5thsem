   "source": [
    "import pandas as pd\n",
    "\n",
    "# Sample DataFrame\n",
    "data = {'A': [10, 40, 99, 35, 25],\n",
    "        'B': [15, 5, 6, 5, 3]}\n",
    "df = pd.DataFrame(data)\n",
    "\n",
    "# Rank transformation\n",
    "df_rank = df.rank()\n",
    "\n",
    "print(\"Original DataFrame:\")\n",
    "print(df)\n",
    "\n",
    "print(\"\\nDataFrame after Rank Transformation:\")\n",
    "print(df_rank)\n"
   ]
